var vertices = [];

var mouseClicked = function(){
    if(mouseButton === LEFT){
        vertices.push([mouseX, mouseY]);
    } else {
        println("beginShape();");
        for(var i in vertices){
            println("vertex(" + vertices[i][0] + ", " + vertices[i][1] + ");");
        }
        println("endShape();");
    }
};

